{
  "name": "xsframe/think-qrcode",
  "description": "QR code generation for thinkphp6",
  "keywords": [
    "thinkphp",
    "think-qrcode",
    "qrcode",
    "thinkphp6"
  ],
  "homepage": "https://github.com/xingshuos/think-qrcode",
  "type": "think-extend",
  "minimum-stability": "stable",
  "version": "1.0",
  "license": "MIT",
  "authors": [
    {
      "name": "xsframe",
      "email": "786824455@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "topthink/framework": "^6.0.0",
    "ext-gd": "*"
  },
  "autoload": {
    "psr-4": {
      "xsframe\\qrcode\\": "src/"
    }
  },
  "extra": {
    "think": {
      "config": {
        "qrcode": "src/config/qrcode.php"
      }
    }
  }
}