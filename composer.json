{
  "name": "topthink/think",
  "description": "the new thinkphp framework",
  "type": "project",
  "keywords": [
    "framework",
    "thinkphp",
    "ORM"
  ],
  "homepage": "http://thinkphp.cn/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "liu21st",
      "email": "liu21st@gmail.com"
    },
    {
      "name": "yunwuxin",
      "email": "448901948@qq.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "topthink/framework": "^6.0.0",
    "topthink/think-orm": "^2.0",
    "topthink/think-multi-app": "^1.0",
    "topthink/think-view": "^1.0",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-iconv": "*",
    "topthink/think-captcha": "^3.0",
    "ext-exif": "*",
    "ext-xmlwriter": "*",
    "topthink/think-image": "^1.0",
    "ext-mcrypt": "*",
    "ext-zlib": "*",
    "phpoffice/phpspreadsheet": "^1.19",
    "ext-zip": "*",
    "aliyuncs/oss-sdk-php": "^2.4",
    "ext-mysqli": "*",
    "ext-dom": "*",
    "topthink/think-helper": "^3.1",
    "alibabacloud/vod-20170321": "2.16.12",
    "alibabacloud/iot": "^1.8",
    "topthink/think-queue": "^3.0",
    "ext-bcmath": "*",
    "tinywan/antfin-php-sdk": "^0.0.1",
    "phpmailer/phpmailer": "^6.9",
    "workerman/gatewayclient": "^3.1",
    "topthink/think-worker": "^3.0",
    "qiniu/php-sdk": "^7.12",
    "qcloud/cos-sdk-v5": "^2.6",
    "xsframe/think-qrcode": "^1.0",
    "xsframe/framework": "^1.0",
    "ext-fileinfo": "*",
    "workerman/channel": "^1.0",
    "ext-posix": "*"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.2",
    "topthink/think-trace": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app"
    },
    "psr-0": {
      "": "extend/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "scripts": {
    "post-autoload-dump": [
      "@php think service:discover",
      "@php think vendor:publish"
    ]
  },
  "repositories": {
    "xsframe": {
      "type": "vcs",
      "url": "https://gitee.com/xingshuos_admin/xs-chinaums"
    }
  }
}
